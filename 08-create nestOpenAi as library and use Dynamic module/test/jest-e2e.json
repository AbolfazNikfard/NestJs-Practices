{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@AolfazNikfard/nest-open-ai/(.*)": "<rootDir>/../libs/nest-open-ai/src/$1",
    "@AolfazNikfard/nest-open-ai": "<rootDir>/../libs/nest-open-ai/src"
  }
}