{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "nest-open-ai": {
      "type": "library",
      "root": "libs/nest-open-ai",
      "entryFile": "index",
      "sourceRoot": "libs/nest-open-ai/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-open-ai/tsconfig.lib.json"
      }
    }
  }
}